#Z.6

n = int(input())
suma = 0
i = 1
while n > 0:
    if i % 19 == 0:
        suma += i
        n -= 1
    i = i + 1
print(suma,)

#Z.7

NIE WUIEM CZY DOBRZE

n = int(input())
suma = 0
i = 1000
while n > 0:
    if i % 37 == 0:
        suma += i
        n -= 1
    i = i - 1
print(suma,)

#Z.8

n = int(input())
suma = 0
czynnik = - 1
for i in range(n):
    czynnik = czynnik * (-1)
    suma = suma + (3*i+2) * czynnik
print(suma)



